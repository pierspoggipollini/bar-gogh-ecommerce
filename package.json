    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "changeToProd": "firebase use prod",
    "changeToDev": "firebase use default",
    "deploy:staging": "env-cmd -f .env.stag firebase use stag && firebase deploy",
    "deploy:production": "env-cmd -f .env.prod firebase use prod && firebase deploy",
    "build:deploy": "env-cmd -f .env.stag npm run build && firebase deploy -P bar-gogh-project---staging",
    "dev:staging": "env-cmd -f .env.stag vite --mode stag",
    "build:staging": "env-cmd -f .env.stag vite build --mode stag",
    "dev:production": "env-cmd -f .env.prod vite --mode prod",
    "build:production": "env-cmd -f .env.prod vite build --mode prod"
  },
